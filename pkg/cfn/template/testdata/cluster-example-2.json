                "AvailabilityZone": "us-west-2b",
                "CidrBlock": "192.168.32.0/19",
                "Tags": [
                    {
                        "Key": "kubernetes.io/role/elb",
                        "Value": "1"
                    },
                    {
                        "Key": "Name",
                        "Value": {
                            "Fn::Sub": "${AWS::StackName}/SubnetPublicUSWEST2B"
                        }
                    },
                    {
                        "Key": "yor_trace",
                        "Value": "d3e0fde6-fe6e-41a1-8099-046c1a4b1e9a"
                    }
                ],
                "VpcId": {
                    "Ref": "VPC"
